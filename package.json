{
  "name": "@mocoin/api-abstract-client",
  "version": "0.0.0-alpha.9",
  "description": "Mocoin API Abstract Client Library for Javascript",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "files": [
    "docs",
    "example",
    "lib"
  ],
  "directories": {
    "doc": "./docs",
    "lib": "./lib",
    "example": "./example",
    "test": "./test"
  },
  "scripts": {
    "build": "tsc",
    "check": "npm run tslint && nsp check",
    "clean": "rimraf ./lib \"npm-debug.log*\" ./docs ./.nyc_output",
    "doc": "typedoc --out ./docs/ --exclude **/*.spec.ts ./src/index.ts --ignoreCompilerErrors --excludeExternals --mode file",
    "mocha": "mocha",
    "nyc": "nyc mocha \"src/**/*.spec.ts\"",
    "coverage": "npm run nyc | coveralls",
    "snyk": "snyk wizard",
    "test": "npm run check && npm run coverage",
    "tslint": "tslint --project tsconfig.json -c tslint.json --exclude \"**/*.spec.ts\" src/**/*.ts",
    "preversion": "npm run clean && npm run build && npm test && npm run doc",
    "version": "git add -A",
    "postversion": "git push origin --tags",
    "prepublishOnly": "npm run clean && npm run build && npm test && npm run doc"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mocoin/api-abstract-client.git"
  },
  "keywords": [
    "mocoin"
  ],
  "author": {
    "name": "Motionpicture co.,ltd."
  },
  "contributors": [
    {
      "email": "yamazaki@motionpicture.jp",
      "name": "Tetsu Yamazaki"
    }
  ],
  "license": "ISC",
  "devDependencies": {
    "@types/debug": "0.0.30",
    "@types/form-data": "^2.2.1",
    "@types/http-status": "^0.2.30",
    "@types/isomorphic-fetch": "0.0.34",
    "@types/mocha": "^5.2.1",
    "@types/nock": "^9.1.3",
    "@types/open": "0.0.29",
    "@types/power-assert": "^1.5.0",
    "@types/qs": "^6.5.1",
    "@types/sinon": "^4.3.3",
    "coveralls": "^3.0.1",
    "http-server": "^0.10.0",
    "mocha": "^5.2.0",
    "nock": "^9.4.3",
    "nsp": "^3.2.1",
    "nyc": "^12.0.2",
    "open": "0.0.5",
    "power-assert": "^1.5.0",
    "rimraf": "^2.6.2",
    "sinon": "^5.1.1",
    "snyk": "^1.89.1",
    "ts-node": "^7.0.0",
    "tslint": "^5.11.0",
    "tslint-microsoft-contrib": "^5.0.3",
    "typedoc": "^0.11.1",
    "typescript": "^2.9.2"
  },
  "dependencies": {
    "@mocoin/factory": "0.0.0-alpha.10",
    "debug": "^3.1.0",
    "http-status": "^1.1.2",
    "isomorphic-fetch": "^2.2.1",
    "qs": "^6.5.2"
  }
}
